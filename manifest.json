{
  "manifest_version": 2,
  "name": "YorkRMP",
  "description": "RMP extension for YorkU's course page.",
  "version": "1.0",
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://w2prod.sis.yorku.ca/*"],
      "js": ["content.js"]
    }
  ]
}
