{
  "manifest_version": 2,
  "name": "YorkRMP",
  "description": "RMP extension for YorkU's course page.",
  "version": "1.0",
  "permissions": ["activeTab", "https://www.ratemyprofessors.com/"],
  "background": {
    "scripts": ["back.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
